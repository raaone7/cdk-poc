{
	"name": "address-cdk-poc",
	"version": "0.1.0",
	"bin": {
		"address-cdk-poc": "bin/cdk"
	},
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"husky-setup": "chmod ug+x .husky/* && husky install",
		"build": "tsc",
		"watch": "tsc -w",
		"test": "jest",
		"cdk": "cdk",
		"lint": "rome check . --apply",
		"format": "rome format . --write",
		"fix": "pnpm format && pnpm lint",
		"check": "rome ci .",
		"typecheck": "tsc --noEmit -p tsconfig.json",
		"synth": "cdk synth",
		"deploy": "cdk deploy"
	},
	"devDependencies": {
		"@types/jest": "^29.5.3",
		"@types/node": "20.5.1",
		"aws-cdk": "2.92.0",
		"jest": "^29.6.2",
		"rome": "^12.1.3",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"typescript": "~5.1.6"
	},
	"dependencies": {
		"aws-cdk-lib": "2.92.0",
		"constructs": "^10.2.69",
		"husky": "^8.0.3",
		"source-map-support": "^0.5.21"
	},
	"engines": {
		"node": ">=18",
		"pnpm": ">=8"
	},
	"packageManager": "pnpm@8.4.0",
	"volta": {
		"node": "18.16.0",
		"pnpm": "8.6.1"
	}
}